 519:Caterina.c    **** #if !defined(NO_BLOCK_SUPPORT)
 520:Caterina.c    **** /** Reads or writes a block of EEPROM or FLASH memory to or from the appropriate CDC data endpoint,
 521:Caterina.c    ****  *  on the AVR910 protocol command issued.
 522:Caterina.c    ****  *
 523:Caterina.c    ****  *  \param[in] Command  Single character AVR910 protocol command indicating what memory operation t
 524:Caterina.c    ****  */
 525:Caterina.c    **** static void ReadWriteMemoryBlock(const uint8_t Command)
 526:Caterina.c    **** {
 527:Caterina.c    **** 	uint16_t BlockSize;
 528:Caterina.c    **** 	char     MemoryType;
 529:Caterina.c    **** 
 530:Caterina.c    **** 	bool     HighByte = false;
 531:Caterina.c    **** 	uint8_t  LowByte  = 0;
 532:Caterina.c    **** 
 533:Caterina.c    **** 	BlockSize  = (FetchNextCommandByte() << 8);
 534:Caterina.c    **** 	BlockSize |=  FetchNextCommandByte();
 535:Caterina.c    **** 
 536:Caterina.c    **** 	MemoryType =  FetchNextCommandByte();
 537:Caterina.c    **** 
 538:Caterina.c    **** 	if ((MemoryType != 'E') && (MemoryType != 'F'))
 539:Caterina.c    **** 	{
 540:Caterina.c    **** 		/* Send error byte back to the host */
 541:Caterina.c    **** 		WriteNextResponseByte('?');
 542:Caterina.c    **** 
 543:Caterina.c    **** 		return;
 544:Caterina.c    **** 	}
 545:Caterina.c    **** 
 546:Caterina.c    **** 	/* Disable timer 1 interrupt - can't afford to process nonessential interrupts
 547:Caterina.c    **** 	 * while doing SPM tasks */
 548:Caterina.c    **** 	TIMSK1 = 0;
 549:Caterina.c    **** 
 550:Caterina.c    **** 	/* Check if command is to read memory */
 551:Caterina.c    **** 	if (Command == 'g')
 552:Caterina.c    **** 	{		
 553:Caterina.c    **** 		/* Re-enable RWW section */
 554:Caterina.c    **** 		boot_rww_enable();
 555:Caterina.c    **** 
 556:Caterina.c    **** 		while (BlockSize--)
 557:Caterina.c    **** 		{
 558:Caterina.c    **** 			if (MemoryType == 'F')
 559:Caterina.c    **** 			{
 560:Caterina.c    **** 				/* Read the next FLASH byte from the current FLASH page */
 561:Caterina.c    **** 				#if (FLASHEND > 0xFFFF)
 562:Caterina.c    **** 				WriteNextResponseByte(pgm_read_byte_far(CurrAddress | HighByte));
 563:Caterina.c    **** 				#else
 564:Caterina.c    **** 				WriteNextResponseByte(pgm_read_byte(CurrAddress | HighByte));
 565:Caterina.c    **** 				#endif
 566:Caterina.c    **** 
 567:Caterina.c    **** 				/* If both bytes in current word have been read, increment the address counter */
 568:Caterina.c    **** 				if (HighByte)
 569:Caterina.c    **** 				  CurrAddress += 2;
 570:Caterina.c    **** 
 571:Caterina.c    **** 				HighByte = !HighByte;
 572:Caterina.c    **** 			}
 573:Caterina.c    **** 			else
 574:Caterina.c    **** 			{
 575:Caterina.c    **** 				/* Read the next EEPROM byte into the endpoint */
 576:Caterina.c    **** 				WriteNextResponseByte(eeprom_read_byte((uint8_t*)(intptr_t)(CurrAddress >> 1)));
 577:Caterina.c    **** 
 578:Caterina.c    **** 				/* Increment the address counter after use */
 579:Caterina.c    **** 				CurrAddress += 2;
 580:Caterina.c    **** 			}
 581:Caterina.c    **** 		}
 582:Caterina.c    **** 	}
 583:Caterina.c    **** 	else
 584:Caterina.c    **** 	{
 585:Caterina.c    **** 		uint32_t PageStartAddress = CurrAddress;
 586:Caterina.c    **** 
 587:Caterina.c    **** 		if (MemoryType == 'F')
 588:Caterina.c    **** 		{
 589:Caterina.c    **** 			boot_page_erase(PageStartAddress);
 590:Caterina.c    **** 			boot_spm_busy_wait();
 591:Caterina.c    **** 		}
 592:Caterina.c    **** 
 593:Caterina.c    **** 		while (BlockSize--)
 594:Caterina.c    **** 		{
 595:Caterina.c    **** 			if (MemoryType == 'F')
 596:Caterina.c    **** 			{
 597:Caterina.c    **** 				/* If both bytes in current word have been written, increment the address counter */
 598:Caterina.c    **** 				if (HighByte)
 599:Caterina.c    **** 				{
 600:Caterina.c    **** 					/* Write the next FLASH word to the current FLASH page */
 601:Caterina.c    **** 					boot_page_fill(CurrAddress, ((FetchNextCommandByte() << 8) | LowByte));
 602:Caterina.c    **** 
 603:Caterina.c    **** 					/* Increment the address counter after use */
 604:Caterina.c    **** 					CurrAddress += 2;
 605:Caterina.c    **** 				}
 606:Caterina.c    **** 				else
 607:Caterina.c    **** 				{
 608:Caterina.c    **** 					LowByte = FetchNextCommandByte();
 609:Caterina.c    **** 				}
 610:Caterina.c    **** 				
 611:Caterina.c    **** 				HighByte = !HighByte;
 612:Caterina.c    **** 			}
 613:Caterina.c    **** 			else
 614:Caterina.c    **** 			{
 615:Caterina.c    **** 				/* Write the next EEPROM byte from the endpoint */
 616:Caterina.c    **** 				eeprom_write_byte((uint8_t*)((intptr_t)(CurrAddress >> 1)), FetchNextCommandByte());
 617:Caterina.c    **** 
 618:Caterina.c    **** 				/* Increment the address counter after use */
 619:Caterina.c    **** 				CurrAddress += 2;
 620:Caterina.c    **** 			}
 621:Caterina.c    **** 		}
 622:Caterina.c    **** 
 623:Caterina.c    **** 		/* If in FLASH programming mode, commit the page after writing */
 624:Caterina.c    **** 		if (MemoryType == 'F')
 625:Caterina.c    **** 		{
 626:Caterina.c    **** 			/* Commit the flash page to memory */
 627:Caterina.c    **** 			boot_page_write(PageStartAddress);
 628:Caterina.c    **** 
 629:Caterina.c    **** 			/* Wait until write operation has completed */
 630:Caterina.c    **** 			boot_spm_busy_wait();
 631:Caterina.c    **** 		}
 632:Caterina.c    **** 
 633:Caterina.c    **** 		/* Send response byte back to the host */
 634:Caterina.c    **** 		WriteNextResponseByte('\r');
 635:Caterina.c    **** 	}
 636:Caterina.c    **** 
 637:Caterina.c    **** 	/* Re-enable timer 1 interrupt disabled earlier in this routine */	
 638:Caterina.c    **** 	TIMSK1 = (1 << OCIE1A);
 639:Caterina.c    **** }
 640:Caterina.c    **** #endif
