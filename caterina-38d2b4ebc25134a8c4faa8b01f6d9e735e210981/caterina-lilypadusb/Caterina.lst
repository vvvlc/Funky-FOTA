 414:Caterina.c    **** /** Configures all hardware required for the bootloader. */
 415:Caterina.c    **** void SetupHardware(void)
 416:Caterina.c    **** {
 417:Caterina.c    **** 	/* Disable watchdog if enabled by bootloader/fuses */
 418:Caterina.c    **** 	MCUSR &= ~(1 << WDRF);
 419:Caterina.c    **** 	wdt_disable();
 420:Caterina.c    **** 
 421:Caterina.c    **** 	/* Disable clock division */
 422:Caterina.c    **** 	clock_prescale_set(clock_div_1);
 423:Caterina.c    **** 
 424:Caterina.c    **** 	/* Relocate the interrupt vector table to the bootloader section */
 425:Caterina.c    **** 	MCUCR = (1 << IVCE);
 426:Caterina.c    **** 	MCUCR = (1 << IVSEL);
 427:Caterina.c    **** 	
 428:Caterina.c    **** 	LED_SETUP();
 429:Caterina.c    **** 	CPU_PRESCALE(0); 
 430:Caterina.c    **** 	L_LED_OFF();
 431:Caterina.c    **** 	
 432:Caterina.c    **** 	
 433:Caterina.c    **** 	/* Initialize TIMER1 to handle bootloader timeout and LED tasks.  
 434:Caterina.c    **** 	 * With 16 MHz clock and 1/64 prescaler, timer 1 is clocked at 250 kHz
 435:Caterina.c    **** 	 * Our chosen compare match generates an interrupt every 1 ms.
 436:Caterina.c    **** 	 * This interrupt is disabled selectively when doing memory reading, erasing,
 437:Caterina.c    **** 	 * or writing since SPM has tight timing requirements.
 438:Caterina.c    **** 	 */ 
 439:Caterina.c    **** 	OCR1AH = 0;
 440:Caterina.c    **** 	OCR1AL = 250;
 441:Caterina.c    **** 	TIMSK1 = (1 << OCIE1A);					// enable timer 1 output compare A match interrupt
 442:Caterina.c    **** 	TCCR1B = ((1 << CS11) | (1 << CS10));	// 1/64 prescaler on timer 1 input
 443:Caterina.c    **** 
 444:Caterina.c    **** 	/* Initialize USB Subsystem */
 445:Caterina.c    **** 	USB_Init();
 446:Caterina.c    **** }
