 331:Caterina.c    **** /** Main program entry point. This routine configures the hardware required by the bootloader, then
 332:Caterina.c    ****  *  runs the bootloader processing routine until it times out or is instructed to exit.
 333:Caterina.c    ****  */
 334:Caterina.c    **** int main(void)
 335:Caterina.c    **** {
 336:Caterina.c    **** 	/* Save the value of the boot key memory before it is overwritten */
 337:Caterina.c    **** 	uint16_t bootKeyPtrVal = *bootKeyPtr;
 338:Caterina.c    **** 	*bootKeyPtr = 0;
 339:Caterina.c    **** 
 340:Caterina.c    **** 	/* Check the reason for the reset so we can act accordingly */
 341:Caterina.c    **** 	uint8_t  mcusr_state = MCUSR;		// store the initial state of the Status register
 342:Caterina.c    **** 	MCUSR = 0;							// clear all reset flags	
 343:Caterina.c    **** 
 344:Caterina.c    **** 
 345:Caterina.c    **** 
 346:Caterina.c    **** 	/* Watchdog may be configured with a 15 ms period so must disable it before going any further */
 347:Caterina.c    **** 	wdt_disable();
 348:Caterina.c    **** 	
 349:Caterina.c    **** 
 350:Caterina.c    **** 	Dinit();
 351:Caterina.c    **** 	Dputch('\n');
 352:Caterina.c    **** 	Dputch('\r');
 353:Caterina.c    **** 	Dputch('S');
 354:Caterina.c    **** 	Dputh(mcusr_state);//Dputb(mcusr_state);
 355:Caterina.c    **** 
 356:Caterina.c    **** 	/*
 357:Caterina.c    **** 	 * copy internal flash only if reset via watch dog
 358:Caterina.c    **** 	 */
 359:Caterina.c    **** 	if (!(mcusr_state & _BV(EXTRF))) //if not external reset
 360:Caterina.c    **** 	{
 361:Caterina.c    **** 		Dputch('a');
 362:Caterina.c    **** 		if (mcusr_state & _BV(WDRF)) { //if reset by watchdog
 363:Caterina.c    **** 			Dputch('b');
 364:Caterina.c    **** 			copyflashIfValid();
 365:Caterina.c    **** 			Dputch('c');
 366:Caterina.c    **** 		}
 367:Caterina.c    **** 	}
 368:Caterina.c    **** 
 369:Caterina.c    **** 	Dputch('d');
 370:Caterina.c    **** 
 371:Caterina.c    **** 
 372:Caterina.c    **** 	if (mcusr_state & (1<<EXTRF)) {
 373:Caterina.c    **** 		// External reset -  we should continue to self-programming mode.
 374:Caterina.c    **** 		Dputch('e');
 375:Caterina.c    **** 	} else if (mcusr_state & (1<<PORF) && pgm_read_word(0) != 0xFFFF) {		
 376:Caterina.c    **** 		// After a power-on reset skip the bootloader and jump straight to sketch 
 377:Caterina.c    **** 		// if one exists.
 378:Caterina.c    **** 		Dputch('f');
 379:Caterina.c    **** 		StartSketch();
 380:Caterina.c    **** 		Dputch('g');
 381:Caterina.c    **** 	} else if ((mcusr_state & (1<<WDRF)) && (bootKeyPtrVal != bootKey) && (pgm_read_word(0) != 0xFFFF)
 382:Caterina.c    **** 		// If it looks like an "accidental" watchdog reset then start the sketch.
 383:Caterina.c    **** 		Dputch('h');
 384:Caterina.c    **** 		StartSketch();
 385:Caterina.c    **** 		Dputch('i');
 386:Caterina.c    **** 	}
 387:Caterina.c    **** 	
 388:Caterina.c    **** 	Dputch('j');
 389:Caterina.c    **** 	/* Setup hardware required for the bootloader */
 390:Caterina.c    **** 	SetupHardware();
 391:Caterina.c    **** 
 392:Caterina.c    **** 	/* Enable global interrupts so that the USB stack can function */
 393:Caterina.c    **** 	sei();
 394:Caterina.c    **** 	
 395:Caterina.c    **** 	Timeout = 0;
 396:Caterina.c    **** 
 397:Caterina.c    **** 	while (RunBootloader)
 398:Caterina.c    **** 	{
 399:Caterina.c    **** 		CDC_Task();
 400:Caterina.c    **** 		USB_USBTask();
 401:Caterina.c    **** 		/* Time out and start the sketch if one is present */
 402:Caterina.c    **** 		if (Timeout > TIMEOUT_PERIOD)
 403:Caterina.c    **** 			RunBootloader = false;
 404:Caterina.c    **** 
 405:Caterina.c    **** 		LEDPulse();
 406:Caterina.c    **** 	}
 407:Caterina.c    **** 	/* Disconnect from the host - USB interface will be reset later along with the AVR */
 408:Caterina.c    **** 	USB_Detach();
 409:Caterina.c    **** 
 410:Caterina.c    **** 	/* Jump to beginning of application space to run the sketch - do not reset */	
 411:Caterina.c    **** 	StartSketch();
 412:Caterina.c    **** }
