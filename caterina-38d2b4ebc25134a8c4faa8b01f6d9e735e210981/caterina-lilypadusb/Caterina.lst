 215:Caterina.c    **** // Initialize the UART
 216:Caterina.c    **** //void avr_uart_init(void) __attribute__ ((inline));
 217:Caterina.c    **** void avr_uart_init(void)
 218:Caterina.c    **** {
 219:Caterina.c    **** 	     // Prescale Timer 0 to divide by 64
 220:Caterina.c    **** 	     TCCR0B |= _BV(CS01) | _BV(CS00);
 221:Caterina.c    **** 	     // Enable Timer 0 overflow interrupt
 222:Caterina.c    **** 	     TIMSK0 |= _BV(TOIE0);
 223:Caterina.c    **** 
 224:Caterina.c    **** 
 225:Caterina.c    ****   // Enable bidirectional UART
 226:Caterina.c    ****   UCSR1B |= _BV(RXEN1) | _BV(TXEN1);
 227:Caterina.c    ****   // Use 8-bit characters
 228:Caterina.c    ****   UCSR1C |= _BV(UCSZ10) | _BV(UCSZ11);
 229:Caterina.c    ****   // Set the Baud rate
 230:Caterina.c    ****   UBRR1H = (UBRR_VAL >> 8);
 231:Caterina.c    ****   UBRR1L = UBRR_VAL;
 232:Caterina.c    **** }
