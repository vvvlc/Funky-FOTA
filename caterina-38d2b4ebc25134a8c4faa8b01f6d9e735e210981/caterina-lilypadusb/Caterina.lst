  88:Caterina.c    **** /*	Breathing animation on L LED indicates bootloader is running */
  89:Caterina.c    **** uint16_t LLEDPulse;
  90:Caterina.c    **** void LEDPulse(void)
  91:Caterina.c    **** {
  92:Caterina.c    **** 	LLEDPulse++;
  93:Caterina.c    **** 	uint8_t p = LLEDPulse >> 8;
  94:Caterina.c    **** 	if (p > 127)
  95:Caterina.c    **** 		p = 254-p;
  96:Caterina.c    **** 	p += p;
  97:Caterina.c    **** 	if (((uint8_t)LLEDPulse) > p)
  98:Caterina.c    **** 		L_LED_OFF();
  99:Caterina.c    **** 	else
 100:Caterina.c    **** 		L_LED_ON();
 101:Caterina.c    **** }
