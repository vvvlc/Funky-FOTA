 478:Caterina.c    **** /** Event handler for the USB_ControlRequest event. This is used to catch and process control reque
 479:Caterina.c    ****  *  the device from the USB host before passing along unhandled control requests to the library for
 480:Caterina.c    ****  *  internally.
 481:Caterina.c    ****  */
 482:Caterina.c    **** void EVENT_USB_Device_ControlRequest(void)
 483:Caterina.c    **** {
 484:Caterina.c    **** 	/* Ignore any requests that aren't directed to the CDC interface */
 485:Caterina.c    **** 	if ((USB_ControlRequest.bmRequestType & (CONTROL_REQTYPE_TYPE | CONTROL_REQTYPE_RECIPIENT)) !=
 486:Caterina.c    **** 	    (REQTYPE_CLASS | REQREC_INTERFACE))
 487:Caterina.c    **** 	{
 488:Caterina.c    **** 		return;
 489:Caterina.c    **** 	}
 490:Caterina.c    **** 
 491:Caterina.c    **** 	/* Process CDC specific control requests */
 492:Caterina.c    **** 	switch (USB_ControlRequest.bRequest)
 493:Caterina.c    **** 	{
 494:Caterina.c    **** 		case CDC_REQ_GetLineEncoding:
 495:Caterina.c    **** 			if (USB_ControlRequest.bmRequestType == (REQDIR_DEVICETOHOST | REQTYPE_CLASS | REQREC_INTERFACE)
 496:Caterina.c    **** 			{
 497:Caterina.c    **** 				Endpoint_ClearSETUP();
 498:Caterina.c    **** 
 499:Caterina.c    **** 				/* Write the line coding data to the control endpoint */
 500:Caterina.c    **** 				Endpoint_Write_Control_Stream_LE(&LineEncoding, sizeof(CDC_LineEncoding_t));
 501:Caterina.c    **** 				Endpoint_ClearOUT();
 502:Caterina.c    **** 			}
 503:Caterina.c    **** 
 504:Caterina.c    **** 			break;
 505:Caterina.c    **** 		case CDC_REQ_SetLineEncoding:
 506:Caterina.c    **** 			if (USB_ControlRequest.bmRequestType == (REQDIR_HOSTTODEVICE | REQTYPE_CLASS | REQREC_INTERFACE)
 507:Caterina.c    **** 			{
 508:Caterina.c    **** 				Endpoint_ClearSETUP();
 509:Caterina.c    **** 
 510:Caterina.c    **** 				/* Read the line coding data in from the host into the global struct */
 511:Caterina.c    **** 				Endpoint_Read_Control_Stream_LE(&LineEncoding, sizeof(CDC_LineEncoding_t));
 512:Caterina.c    **** 				Endpoint_ClearIN();
 513:Caterina.c    **** 			}
 514:Caterina.c    **** 
 515:Caterina.c    **** 			break;
 516:Caterina.c    **** 	}
 517:Caterina.c    **** }
