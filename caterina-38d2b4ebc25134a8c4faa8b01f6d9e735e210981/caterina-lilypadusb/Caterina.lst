 104:Caterina.c    **** /*
 105:Caterina.c    ****  * Separate function for doing spm stuff
 106:Caterina.c    ****  * It's needed for application to do SPM, as SPM instruction works only
 107:Caterina.c    ****  * from bootloader.
 108:Caterina.c    ****  *
 109:Caterina.c    ****  * How it works:
 110:Caterina.c    ****  * - do SPM
 111:Caterina.c    ****  * - wait for SPM to complete
 112:Caterina.c    ****  * - if chip have RWW/NRWW sections it does additionaly:
 113:Caterina.c    ****  *   - if command is WRITE or ERASE, AND data=0 then reenable RWW section
 114:Caterina.c    ****  *
 115:Caterina.c    ****  * In short:
 116:Caterina.c    ****  * If you play erase-fill-write, just set data to 0 in ERASE and WRITE
 117:Caterina.c    ****  * If you are brave, you have your code just below bootloader in NRWW section
 118:Caterina.c    ****  *   you could do fill-erase-write sequence with data!=0 in ERASE and
 119:Caterina.c    ****  *   data=0 in WRITE
 120:Caterina.c    ****  *
 121:Caterina.c    ****  * How to find address of this function?
 122:Caterina.c    ****  * You have to generate listing of bootloader ie. Catalina.lss search for <__do_spm>
 123:Caterina.c    ****  * 000070ac <__do_spm>:    where  0x70ac is the entry point.
 124:Caterina.c    ****  * it is before vector_ends function 0x70ac
 125:Caterina.c    ****  *
 126:Caterina.c    ****  * in your scatch file use this to
 127:Caterina.c    ****  * typedef void (*do_spm_t)(uint16_t address, uint8_t command, uint16_t data);
 128:Caterina.c    ****  * #define SLLOCJMP ((do_spm_t)(0x70ac>>1))
 129:Caterina.c    ****  * const do_spm_t do_spm = SLLOCJMP;
 130:Caterina.c    ****  *
 131:Caterina.c    ****  * The same as do_spm but with disable/restore interrupts state
 132:Caterina.c    ****  * required to succesfull SPM execution
 133:Caterina.c    ****  *
 134:Caterina.c    ****  * void do_spm_cli(uint16_t address, uint8_t command, uint16_t data) {
 135:Caterina.c    ****  *   uint8_t sreg_save;
 136:Caterina.c    ****  *
 137:Caterina.c    ****  *   sreg_save = SREG;  // save old SREG value
 138:Caterina.c    ****  *   asm volatile("cli");  // disable interrupts
 139:Caterina.c    ****  *   do_spm(address,command,data);
 140:Caterina.c    ****  *   SREG=sreg_save; // restore last interrupts state
 141:Caterina.c    ****  * }
 142:Caterina.c    ****  *
 143:Caterina.c    ****  */
 144:Caterina.c    **** 
 145:Caterina.c    **** void __do_spm(uint16_t address, uint8_t command, uint16_t data) __attribute__ ((used, section (".ve
 146:Caterina.c    **** void __do_spm(uint16_t address, uint8_t command, uint16_t data) {
 147:Caterina.c    ****     // Do spm stuff
 148:Caterina.c    ****     __asm__ volatile (
 149:Caterina.c    ****         "    movw  r0, %3\n"
 150:Caterina.c    ****         "    out %0, %1\n"
 151:Caterina.c    ****         "    spm\n"
 152:Caterina.c    ****         "    clr  r1\n"
 153:Caterina.c    ****         :
 154:Caterina.c    ****         : "i" (_SFR_IO_ADDR(__SPM_REG)),
 155:Caterina.c    ****           "r" ((uint8_t)command),
 156:Caterina.c    ****           "z" ((uint16_t)address),
 157:Caterina.c    ****           "r" ((uint16_t)data)
 158:Caterina.c    ****         : "r0"
 159:Caterina.c    ****     );
 160:Caterina.c    **** 
 161:Caterina.c    ****     // wait for spm to complete
 162:Caterina.c    ****     //   it doesn't have much sense for __BOOT_PAGE_FILL,
 163:Caterina.c    ****     //   but it doesn't hurt and saves some bytes on 'if'
 164:Caterina.c    ****     boot_spm_busy_wait();
 165:Caterina.c    **** #if defined(RWWSRE)
 166:Caterina.c    ****     // this 'if' condition should be: (command == __BOOT_PAGE_WRITE || command == __BOOT_PAGE_ERASE
 167:Caterina.c    ****     // but it's tweaked a little assuming that in every command we are interested in here, there
 168:Caterina.c    ****     // must be also SELFPRGEN set. If we skip checking this bit, we save here 4B
 169:Caterina.c    ****     if ((command & (_BV(PGWRT)|_BV(PGERS))) && (data == 0) ) {
 170:Caterina.c    ****       // Reenable read access to flash
 171:Caterina.c    ****       boot_rww_enable();
 172:Caterina.c    ****     }
 173:Caterina.c    **** #endif
 174:Caterina.c    **** }
