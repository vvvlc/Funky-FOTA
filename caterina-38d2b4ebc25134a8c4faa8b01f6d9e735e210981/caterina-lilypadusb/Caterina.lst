 275:Caterina.c    **** typedef struct flash_header
 276:Caterina.c    **** {
 277:Caterina.c    ****  unsigned int crc:5;//cksum
 278:Caterina.c    ****  unsigned int compressed:1;//1 - compressed image
 279:Caterina.c    ****  unsigned int size:10;//length of flash
 280:Caterina.c    **** }FLASH_HEADER;
 281:Caterina.c    **** #define NEW_FLASH_OFFSET 0x3800
 282:Caterina.c    **** #define SIZE_OF_NEW_FLASH_HEADER 2
 283:Caterina.c    **** /**
 284:Caterina.c    ****  * returns 0 sucessfull copy of new flash
 285:Caterina.c    ****  * 			1 illegal size of new flash
 286:Caterina.c    ****  */
 287:Caterina.c    **** int copyflashIfValid(void) {
 288:Caterina.c    **** 	uint16_t i,page;
 289:Caterina.c    **** 	uint16_t imagesize=0x7FFF;
 290:Caterina.c    **** 
 291:Caterina.c    **** 	//read image size if size is not valid (ignore it)
 292:Caterina.c    **** 	imagesize=pgm_read_word(NEW_FLASH_OFFSET);
 293:Caterina.c    **** 	Dputch('F');
 294:Caterina.c    **** 	if (imagesize>=NEW_FLASH_OFFSET || imagesize == 0) {
 295:Caterina.c    **** 		Dputch('1');
 296:Caterina.c    **** 		return 1;
 297:Caterina.c    **** 	}
 298:Caterina.c    **** 
 299:Caterina.c    **** 	Dputch('2');
 300:Caterina.c    **** 	for(i=0;i<imagesize;) {
 301:Caterina.c    **** 		page=i;
 302:Caterina.c    **** 		Dputch('*');
 303:Caterina.c    **** 		for (; i<page+SPM_PAGESIZE; i+=2)
 304:Caterina.c    **** 		{
 305:Caterina.c    **** 			// Set up little-endian word.
 306:Caterina.c    **** 			uint16_t w = pgm_read_word(i + NEW_FLASH_OFFSET+SIZE_OF_NEW_FLASH_HEADER);
 307:Caterina.c    **** 			boot_page_fill (i, w);
 308:Caterina.c    **** 		}
 309:Caterina.c    **** 		boot_page_erase(page);
 310:Caterina.c    **** 		boot_spm_busy_wait();
 311:Caterina.c    **** 		boot_page_write (page); // Store buffer in flash page.
 312:Caterina.c    **** 		boot_spm_busy_wait();
 313:Caterina.c    **** #if defined(RWWSRE)
 314:Caterina.c    **** 		// Reenable read access to flash
 315:Caterina.c    **** 		boot_rww_enable();
 316:Caterina.c    **** #endif
 317:Caterina.c    **** 	}
 318:Caterina.c    **** 
 319:Caterina.c    **** 	/*
 320:Caterina.c    **** 	 * erase first page as sign of program flashed
 321:Caterina.c    **** 	 */
 322:Caterina.c    **** 	Dputch('3');
 323:Caterina.c    **** 	boot_page_erase(NEW_FLASH_OFFSET);
 324:Caterina.c    **** 	boot_spm_busy_wait();
 325:Caterina.c    **** //    TIMSK1 = (1 << OCIE1A);
 326:Caterina.c    ****     //now trigger a watchdog reset
 327:Caterina.c    ****     watchdogConfig(WATCHDOG_16MS);  // short WDT timeout
 328:Caterina.c    **** 	while (1) {Dputch('-');}; 		                  // and busy-loop so that WD causes a reset and app s
 329:Caterina.c    **** }
