 236:Caterina.c    **** 	  // Wait to be able to transmit
 237:Caterina.c    **** 	  while((UCSR1A & _BV(UDRE1)) == 0) ;
 238:Caterina.c    **** 	  // Put the data into the send buffer
 239:Caterina.c    **** 	  UDR1 = ch;
 240:Caterina.c    **** 
 241:Caterina.c    **** 	  _delay_ms(1000);
 242:Caterina.c    **** }
