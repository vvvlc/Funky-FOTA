  68:Caterina.c    **** void StartSketch(void)
  69:Caterina.c    **** {
  70:Caterina.c    **** 	cli();
  71:Caterina.c    **** 	
  72:Caterina.c    **** 	/* Undo TIMER1 setup and clear the count before running the sketch */
  73:Caterina.c    **** 	TIMSK1 = 0;
  74:Caterina.c    **** 	TCCR1B = 0;
  75:Caterina.c    **** 	TCNT1H = 0;		// 16-bit write to TCNT1 requires high byte be written first
  76:Caterina.c    **** 	TCNT1L = 0;
  77:Caterina.c    **** 	
  78:Caterina.c    **** 	/* Relocate the interrupt vector table to the application section */
  79:Caterina.c    **** 	MCUCR = (1 << IVCE);
  80:Caterina.c    **** 	MCUCR = 0;
  81:Caterina.c    **** 
  82:Caterina.c    **** 	L_LED_OFF();
  83:Caterina.c    **** 	
  84:Caterina.c    **** 	/* jump to beginning of application space */
  85:Caterina.c    **** 	__asm__ volatile("jmp 0x0000");
  86:Caterina.c    **** }
